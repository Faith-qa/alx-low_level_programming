#include "holberton.h"
#include <stdio.h>

/**
 *_strcat - concatenates 2 stings
 *@dest: destination array
 *@src: starting character
 *Return: dest
 */
char *_strcat(char *dest, char *src)
{
int a, b;
a = 0;
while (dest[a] != '\0')
{
a++;
}
b = 0;
while (src[b] != '\0')
{
dest[a] = src[b];
b++;
a++;
 }
return (dest);
}
