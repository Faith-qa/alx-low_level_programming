#include "holberton.h"
#include <stdio.h>
/**
 *_sqrt_recursion- returns a natural squareroot of a number
 *@n: integer n
 *Return: -1 n is not a natural squareroot
 */
int _sqrt_recursion(int n)
{
}
/**
 * sqrt_value - recursion function to find square root
 * @a: integer to be squared
 * @b: integer that is square of @a
 * Return: Integer
 */

int sqrt_value(int a, int b)
{
	if (a * a > b)
	{
		return (-1);
	}
	if (a * a == b)
	{
		return (a);
	}
	else
		return (sqrt_value((a + 1), b));
}
